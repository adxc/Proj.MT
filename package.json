{
	"name": "vite-project",
	"private": true,
	"version": "0.0.0",
	"main": "main.js",
	"author": "wjh",
	"description": "MT项目管理",
	"homepage": "./",
	"scripts": {
		"dev": "vite",
		"start": "electron .",
		"build": "tsc && vite build&&cp -r server build&&cp -R mainBuild.js build",
		"buildWin": "tsc && vite build&&robocopy server build&&robocopy mainBuild.js build",
		"preview": "vite preview",
		"rebuild": "electron-rebuild -f -w node-pty",
		"pack": "electron-builder --dir",
		"postinstall": "electron-builder install-app-deps",
		"dist": "electron-builder",
		"dist-win": "electron-builder  --win --ia32",
		"dist-win64": "electron-builder  --win --x64",
		"dist-mac": "electron-builder --mac",
		"dist-linux": "electron-builder --linux",
		"package": "electron-builder build --mac --win --x64 -c.extraMetadata.main=build/electron.js --publish never"
	},
	"build": {
		"appId": "react.example.com",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"publish": {
			"provider": "github",
			"repo": "electron-cra -示例",
			"owner": "wjh"
		},
		"mac": {
			"target": [
				"dmg",
				"zip"
			]
		},
		"win": {
			"target": [
				"nsis",
				"zip"
			]
		},
		"nsis": {
			"guid": "JENSONJINELECTRON",
			"deleteAppDataOnUninstall": true
		},
		"asar": false
	},
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@fontsource/roboto": "^4.5.3",
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-regular-svg-icons": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"@mui/icons-material": "^5.6.2",
		"@mui/material": "^5.7.0",
		"@reduxjs/toolkit": "^1.8.2",
		"chalk": "^5.0.1",
		"classnames": "^2.3.1",
		"electron-store": "^8.0.1",
		"node-pty": "^0.10.1",
		"normalize.css": "^8.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^8.0.2",
		"react-spring": "^9.4.5",
		"react-virtualized-auto-sizer": "^1.0.6",
		"react-window": "^1.8.7",
		"redux": "^4.2.0",
		"shelljs": "^0.8.5",
		"uuid": "^8.3.2",
		"xterm": "^4.18.0",
		"xterm-addon-fit": "^0.5.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.4",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"@types/react-window": "^1.8.5",
		"@types/uuid": "^8.3.4",
		"@vitejs/plugin-react": "^1.3.2",
		"autoprefixer": "^10.4.7",
		"electron": "^18.2.3",
		"electron-builder": "^23.1.0",
		"electron-rebuild": "^3.2.7",
		"postcss": "^8.4.13",
		"tailwindcss": "^3.0.24",
		"typescript": "^4.6.4",
		"vite": "^2.9.9"
	}
}
